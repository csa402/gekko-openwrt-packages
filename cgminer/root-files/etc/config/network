config interface 'loopback'
        option ifname 'lo'
        option proto 'static'
        option ipaddr '127.0.0.1'
        option netmask '255.255.255.0'

config interface 'lan'
        option ifname 'eth0'
        option type 'bridge'
        option _orig_ifname 'eth0 radio0.network1'
        option _orig_bridge 'true'
        option proto 'dhcp'
        option hostname 'OpenWRT'
